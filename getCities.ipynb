   "source": [
    "import pandas as pd\n",
    "df = pd.read_csv(\"cleansed_data.csv\")\n",
    "cities = set()\n",
    "for i in range(0,len(df)-1):\n",
    "    cities.add(df.at[i,'City'])\n",
    "\n",
    "with open('cities.txt', 'w') as f:\n",
    "    for city in cities:\n",
    "        f.write(city)\n",
    "        f.write(\"\\n\")"
   ]
  }
